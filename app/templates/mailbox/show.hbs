<div class="row">
  <div class="col-sm-12">
    <h1>{{model.name}}</h1>
  </div>
</div>

<div class="row">
  <div class="col-sm-3">
    <ul>
      {{#each mail in mails}}
      <li>
        {{#link-to 'mailbox.show.mail' mail}}{{mail.updated_at}}{{/link-to}}
      </li>
      {{/each}}
    </ul>
  </div>
  <div class="col-sm-9">
    {{outlet}}
  </div>
</div>
